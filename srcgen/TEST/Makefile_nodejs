######
#
# For testing xats2js
#
######
CAT=cat
CPF=cp -f
ECHO=echo
NODE=node
######
SHARE=./../../share
XATS2JS="./../../bin/xats2js"
######
XATS2JS_PRELUDE=\
"$(SHARE)/runtime/xats2js_prelude.js"
XATS2JS_PRELUDE_NODE=\
"$(SHARE)/runtime/xats2js_prelude_node.js"
######
all:: test
######
PRELUDE=./prelude
GITHWXI=./xatslib_githwxi
######
test:: \
eqref
eqref:: ; \
$(CAT) $(XATS2JS_PRELUDE) > output_dats.js && \
$(CAT) $(XATS2JS_PRELUDE_NODE) >> output_dats.js && \
$(XATS2JS) --output-a output_dats.js -d $(PRELUDE)/eqref.dats && $(NODE) output_dats.js
######
test:: \
sieve
sieve:: ; \
$(CAT) $(XATS2JS_PRELUDE) > output_dats.js && \
