CAT=cat
CPF=cp -f
ECHO=echo
NODE=node

######
SHARE=./../../share
XANADU=./../../xanadu
XATS2JS="./../../bin/xats2js"
######
XATS2JS_RUNTIME=\
"$(SHARE)/runtime/xats2js_runtime.js"
######
all:: test
######
PRELUDE=./prelude
GITHWXI=./xatslib_githwxi
######
test:: \
test-2020-03-21
test-2020-03-21:: ; \
$(CAT) $(XATS2JS_RUNTIME) > output_dats.js && \
$(XATS2JS) --output-a output_dats.js -d $(GITHWXI)/test-2020-03-21.dats && $(NODE) output_dats.js
######
