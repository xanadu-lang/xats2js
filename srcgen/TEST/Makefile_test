all::
######
MAKE=make
######
all:: \
test_output
test_output:: ; \
$(MAKE) -f Makefile_output test cleanall > /dev/null
######
all:: \
test_nodejs
test_nodejs:: ; \
$(MAKE) -f Makefile_nodejs test cleanall > /dev/null
######
#
# HX: for cleaning up
#
######
clean:: ; rm -f *~
######
cleanall:: clean
cleanall:: ; rm -f output
cleanall:: ; rm -f output_dats.js
######
###### end of [Makefile_test] ######
